
package com.nqt.array;

public class findcroudoncruise {
	public static void main(String args[]) {

		int T = 5;
		int E[] = { 7, 0, 5, 1, 3 };
		int L[] = { 1, 2, 1, 3, 4 };
		int sum=0;
		int guest=0;

		for (int i = 0; i < T; i++) {
			sum+=E[i];
			sum-=L[i];
			
			if(sum>guest) {
				guest=sum;
			}
			
			System.out.println(sum+" are in cruise in hr "+i);
		}
		
		System.out.println(guest+" are in cruise in hr ");
	}
}
